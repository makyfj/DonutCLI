{
  "name": "donutcli",
  "version": "1.0.0",
  "description": "Small Donut CLI",
  "main": "./lib/index.js",
  "bin": {
    "donut": "./lib/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "local": "sudo npm i -g && donut",
    "refresh": "rm -rf ./node_module ./package-lock.json && npm install"
  },
  "keywords": [
    "donut",
    "cli"
  ],
  "author": "makyfj",
  "license": "ISC",
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^8.1.0",
    "figlet": "^1.5.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.4",
    "@types/node": "^16.4.13",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  }
}
